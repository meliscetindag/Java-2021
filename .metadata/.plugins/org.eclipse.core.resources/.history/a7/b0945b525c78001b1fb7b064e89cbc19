package csd;

public class App {
	public static void main(String[] args)
	{
        java.util.Scanner kb = new java.util.Scanner(System.in);
		
		System.out.print("Bir sayÄ± giriniz:");
		int val = Integer.parseInt(kb.nextLine());
		
		int square = val * val;
		
		System.out.println(square);	
		
		kb.close();
	}

}
